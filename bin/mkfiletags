#!/bin/sh
# vim:ft=sh:ts=2:sw=2:et:tw=0:

echo "!_TAG_FILE_SORTED    2   /2=foldcase/"
find . \
  \( -name .git \) -prune -o \
  -not -iregex '.*\.\(jar\|gif\|jpg\|class\|exe\|dll\|pdb\|sw[op]\|xlsx\|docx\|pdf\|zip\|tar\|ico\|ear\|war\|dat\|o\|obj\|a\|lib\)' \
  -type f -printf "%f\t%p\t1\n" | sort -f

